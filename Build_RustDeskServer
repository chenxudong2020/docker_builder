
FROM ubuntu:20.04
WORKDIR /app


RUN echo "deb http://th.archive.ubuntu.com/ubuntu jammy main" >> /etc/apt/sources.list

# apt
RUN apt-get update && \
    apt-get install -y openssl curl && \
    apt-get install -y libc6

RUN chmod +x rustdesk/hbbr rustdesk/hbbs rustdesk/runhbb rustdesk/stophbb

COPY rustdesk /app 

RUN ll -al /app/rustdesk

CMD bash /app/rustdesk/runhbb  
