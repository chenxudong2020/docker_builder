
FROM visense/debain11_slim



ADD rustdesk /usr/local/rustdesk

 RUN cd /usr/local/rustdesk && \ 
     chmod +x hbbs hbbr rustdesk-utils runhbb rustdeskweb apiserver

  

EXPOSE 21114
EXPOSE 21115
EXPOSE 21116
EXPOSE 21117
EXPOSE 21118
EXPOSE 21119
EXPOSE 21116/udp


ENV KEY=rustdeskkey


ENTRYPOINT /usr/local/rustdesk/runhbb $KEY false
